<table>
    <tr>
      <th>Match ID</th>
      <th>Team1</th>
      <th>Group Team1</th>
      <th>Team2</th>
      <th>Group Team2</th>
      <th>Team1 score</th>
      <th>Team2 score</th>

    </tr>
      <% for match in matches_group do %>
      <% if match.team_matches.first %>

        <tr>
          <td><%= match.id %></td>
          <td><%= match.team_matches.first.team.name %></td>
          <td><%= match.team_matches.first.team.group_name %></td>
          <td><%= match.team_matches.last.team.name  %> </td>
          <td><%= match.team_matches.last.team.group_name %></td>
          <td><%= match.team_matches.first.score %></td>
          <td><%= match.team_matches.last.score  %> </td>
        </tr>
      <% end %>
    <% end %>

  </table>
  <br>